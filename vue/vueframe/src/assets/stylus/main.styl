
.ttv-table
  border 1px solid #ccc
  border-collapse collapse
  border-spacing 0
  width 100%
  .ttv-table-bigtd
    width 30%
  .ttv-table-orgtd
    width 20%
  .ttv-table-midtd
    width 170px
  thead
    td
      background-color #FCFCFC
      font-size 12px
      font-weight bold
  tr
    height: 25px;
    line-height 25px
    td
      padding: 4px 20px;
      border: 1px solid #ddd
      input[type="checkbox"]
        margin-right 5px
    &:hover
      background-color #E5FCFD
  tbody
    tr
      &:hover
        .ttv-btn
          display inline
      &:nth-child(even)
        background-color : #FCFCFC
        &:hover
          background-color #E5FCFD
.ttv-table-big
  tr
    td
      padding 4px 10px
      max-width 180px
      word-wrap break-word
      img
        width 50px
        height 30px
        margin-top -3px
        margin-bottom -3px

.ttv-model-table
  tr
    td
      &:nth-child(odd)
        text-align right
      overflow hidden
      word-wrap  break-word
      &:nth-child(even)
        max-width 300px



/*-------------panel----------*/

.ttv-col-xs12,.ttv-col-xs9,.ttv-col-xs8,.ttv-col-xs10,.ttv-col-xs6,.ttv-col-xs3,.ttv-col-xs2,.ttv-col-xs7,.ttv-col-xs4
  position relative
  padding 0 10px
  padding-right: 15px;
  padding-left: 15px;
  min-height 1px

.ttv-col-xs12
  width 100%
.ttv-col-xs10
  width 80%
.ttv-col-xs9
  width 75%
.ttv-col-xs8
  width 70%
.ttv-col-xs7
  width 60%
.ttv-col-xs6
  width 50%
.ttv-col-xs4
  width 40%
.ttv-col-xs3
  width 30%
.ttv-col-xs25
  width 25%
.ttv-col-xs2
  width 20%

.ttv-panel
  width 100%
  margin-bottom: 20px;
  /*background-color: #fff;*/
  /*background-color #eee*/
  border: 1px solid transparent;
  border-radius: 4px;
  -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, .05);
  box-shadow: 0 1px 1px rgba(0, 0, 0, .05);
  min-height 20px
.ttv-panel-primary
  border-color primary_color
  .ttv-panel-header
    background-color primary_color

.ttv-panel-header
  /*min-height 40px*/
  padding: 7px 15px
  width 100%
  color #fff
  .ttv-header-search
    margin-top -7px
    padding-top 3px
    margin-bottom -7px
    input[type="text"]
        margin-right 5px
        border-radius 6px
    .ttv-btn
      margin 0
    .ttv-select-form
      margin-right 20px
  .ttv-panel-option
  .ttv-panel-title
    margin-top: 0
    margin-bottom: 0
    font-size: 14px
  &:after
    content " "
    display block
    clear both
.ttv-panel-body
  padding 15px 10px

/*--------- panel 结束 ------------*/


.ttv-btn
    margin 20px 4px
    padding 5px 8px
    background-color primary_color
    min-width 40px
    text-align center
    color white
    border-radius 4px
    border 1px solid #285e8e
    cursor pointer
    font-weight 400
    /*letter-spacing 1px*/
    &:hover
      background-color #3276b1
.ttv-btn-success
  background-color #12c712
  border 1px solid #0e8e0d

.ttv-btn-disabled
  background-color #949393
  border 1px solid #8C8C8C
  &:hover
    background-color #949393

.ttv-form-cancel
  background-color #fff
  border 1px solid #ccc
  color #000
  &:hover
    background-color #e6e6e6
    border-color #adadad


.ttv-select
  height 22px
  min-width 110px
  border 1px solid #cccccc

.ttv-select-form
  min-width 180px
  height 25px
  border 1px solid #cccccc
  margin-right 5px

.ttv-prompt
  min-height 35px
  background-color #F9F9F9
  border-radius 3px
  margin 5px 0
  padding 5px 20px
  position relative
  em
    color #ff0000
    padding 0 5px

.ttv-prompt-head
  margin-top 0px
  line-height 35px
  height 35px
  padding 0 20px

  .ttv-btn
    margin 0
    margin-left 10px
  select,input,button
    margin-left 10px
  .ttv-form-control
    width 60%
  .ttv-select-form
    width auto
    min-width 80px


.ttv-search-info
  background-color #FFFFCB
  margin-top -5px
  margin-bottom 5px
  color #40B817
  _transition(.7s)
  /*display none*/
  opacity 0
  height 0
  position relative

.ttv-search-info-show
  /*display block*/
  opacity 0.9
  height 35px

.ttv-search-btn
  position absolute
  right 30px
  top 5px
  min-width 50px
  letter-spacing 3px
.ttv-search-btn2
  position absolute
  right 100px
  top 5px
  letter-spacing 3px

/*------------表单内容--------------*/
.ttv-form-control
  height 30px
  border 1px solid #cccccc
  background-color #fff
  color #000000
  border-radius 2px
  padding 6px 12px
  font-size 12px
.ttv-form-control-big
  width 350px !important
.ttv-form-line
  .ttv-form-group
    display inline-block
    margin-bottom 0
    vertical-align middle
  .ttv-form-control
    display inline-block
    vertical-align middle
    width auto

.ttv-form-group
  margin-bottom 15px
  label
    margin-right 3px
  input,select
    margin-right 5px
  .ttv-form-group-tips
    color red
    max-width 20px
    overflow hidden
  .ttv-form-label
    min-width 70px
  .ttv-checkbox-online ,.ttv-radio-online
      margin-right 20px
.ttv-form-submit
  text-align right
  .ttv-btn
    margin 2px 10px
    letter-spacing 2px
    width 70px
.ttv-form-msg
  color #ff0000
  margin 10px 20px 0 0











//遮罩层
.ttv-model-backdrop
  height 100%
  width 100%
  position fixed
  z-index -1
  background-color: #fff
  opacity 0
  top 0
  display block
  _transition( .6s)
  transition-timing-function : ease-out
.ttv-in
  display block
  z-index 10000
  background-color: #000
  opacity 0.35


/*---------------浮层-------------------*/
/*.ttv-show-model*/
  /*.ttv-model-box*/
    /*top 0*/
/*opacity 1*/
/*.ttv-close-model*/
  /*.ttv-model-box*/
