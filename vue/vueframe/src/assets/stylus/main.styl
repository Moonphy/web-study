.ttv-model-box
  position fixed
  margin auto
  width 500px
  height 350px
  background #fff
  top 0px
  left 0
  z-index 10001
  right 0
  bottom 0
  /*opacity 0*/
  /*padding 10px 25px*/
  border 1px solid #ECECEC
  border-radius 3px;
  box-shadow: 1px 1px 1px #ccc;
  _transition(top, 0.5s)
  .ttv-model-table
    tr
      td:nth-child(odd)
        text-align: left
  .ttv-prompt-head
    .ttv-form-control
      width 25%
  .ttv-box-header
    /*text-align center*/
    padding 0 20px 0 20px
    line-height 45px
    height 45px
    border-bottom 1px solid #ECECEC
    /*box-shadow: 1px 1px 1px;*/
    background #FDFDFD
    h3
      float left
      width 120px
      font-weight bold
    .ttv-box-close
      float right
      font-size 14px
      color #ccc
      _transition( 0.3s)
      &:hover
        color #000011
  .ttv-box-body
    padding 20px

  .ttv-box-footer
    position absolute
    /*opacity 0*/
    /*padding 10px 0*/
    bottom 0
    display block
    height 45px
    /*line-height 50px*/
    border-top 1px solid #ECECEC
    /*padding: 0 20px 0 20px;*/
    width 100%
    .ttv-box-tips
      color #ED0A0A
      margin-left 20px
      float left
      margin-top 11px
    .ttv-box-btns
      /*position absolute*/
      float right
      margin: 6px 10px 0 0
      /*top 2px*/
      .ttv-box-btn
        min-width 60px
        height 32px
        line-height 32px;
        border 1px solid #cccccc
        border-radius 2px;
        margin 0px 5px
        padding 0 3px
        font-size 14px
        &:hover
          background #eee
      .ttv-btn-submit
        background #337ab7
        border 1px solid #2e6da4
        color: #fff
        &:hover
          background #286090
      .ttv-btn-cls
        background #fff
.ttv-model-bigbox
  width 600px
  height 520px
  .ttv-box-body
    height 420px
    overflow scroll
.ttv-model-sm
  height 180px
  width 300px
  .ttv-box-header
    height 35px
    line-height 35px
  .ttv-box-footer
    height 40px
    .ttv-box-btns
      margin: 6px 10px 0 0
      .ttv-box-btn
        height 25px
        line-height 25px
        font-size 12px

/*-----------浮层结束--------------------*/

/*-----顶部提示层------*/
.ttv-top-tips
  position fixed
  background-color #000
  min-height 50px
  width 300px
  right 20px
  text-align center
  padding-top 15px
  opacity 0
  z-index 10010
  /*display none*/
  top -50px
  font-size 14px
  border  1px solid #ccc
  box-shadow  1px 1px 1px #ccc
  border-radius 5px
  color #fff
  font-weight 700
  _transition(.3s)

.tts-top-tips-show


.ttv-top-tips-suc
  background-color #51a351
  display block
  top 20px
  opacity .9

.ttv-top-tips-warning
  background-color #f89406
  display block
  top 20px
  opacity .9
.ttv-top-tips-err
  background-color #bd362f
  display block
  top 20px
  opacity .9
.ttv-top-tips-info
  background-color #2f96b4
  display block
  top 20px
  opacity .9

.ttv-load-img
  position absolute
  top 5px
  width 20px
  height 20px
  margin 2px 5px

.ttv-divitem
  position relative
  .ttv-load-img
    top 20px

 .ttv-img-small{
   width 30px
   height 30px
 }


/*------------------- 分页 ----------------------*/
.ttv-pager
  margin-top 2px
  &:after
    content " "
    display  block
    height 0
    clear both
  .ttv-pagination
    li
      float left
      a
        border 1px solid #ccc
        margin-left 5px
        min-width 40px
        padding-top 2px
        display inline-block
        border-radius 4px
        /*margin-left -1px*/
        line-height: 1.8
        text-align center
        color #337ab7
        background-color #fff
        &:hover
          background-color #FCFCFC
      /*&:first-child>a,&:first-child>span*/
        /*margin-left: 0;*/
        /*border-top-left-radius: 4px;*/
        /*border-bottom-left-radius: 4px;*/

     li.ttv-active
       a
         border 1px solid #337ab7
         background-color #4B9ADF
         color #FFFFFF
         font-weight bold
         &:hover
           background-color #337ab7

     li.ttv-disabled
       a
        color #777


/*------------------- 分页结束 ----------------------*/